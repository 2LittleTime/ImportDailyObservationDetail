                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT
[WBAN],
CAST ([YearMonthDay] as date) AS YearMonthDayStg,
CASE WHEN ISNUMERIC([TMax]) = 0 THEN NULL ELSE [Tmax] END AS TempMaxStg,
CASE WHEN ISNUMERIC([TMin]) = 0 THEN NULL ELSE [Tmin] END AS TempMinStg,
CASE WHEN ISNUMERIC([Tavg]) = 0 THEN NULL ELSE [Tavg] END AS TempAvgStg,
CASE WHEN ISNUMERIC([Water1]) = 0 THEN '0' ELSE [Water1] END AS Water1Stg,
CASE WHEN ISNUMERIC([SnowFall]) = 0 THEN '0' ELSE [SnowFall] END AS SnowFallStg,
CASE WHEN ISNUMERIC([PrecipTotal]) = 0 THEN '0' ELSE [PrecipTotal] END AS PrecipTotalStg,
CASE WHEN ISNUMERIC([AvgSpeed]) = 0  THEN NULL ELSE [AvgSpeed] END AS WindAvgSpeedStg,
CASE WHEN ISNUMERIC([Max5Speed]) = 0 THEN NULL ELSE [Max5Speed] END AS WindMax5SpeedStg,
CASE WHEN ISNUMERIC([Max2Speed]) = 0 THEN NULL ELSE [Max2Speed] END AS WindMax2SpeedStg,
CAST(LEFT([YearMonthDay],4) AS INT) AS YEAR,
CAST(SUBSTRING([YearMonthDay],5,2) AS INT) AS MONTH,
CAST(RIGHT([YearMonthDay],2) AS INT) AS DAY
FROM
[dbo].[DailyTextStg]
WHERE
[WBAN] IS NOT NULL AND
[YearMonthDay] IS NOT NULL
ORDER BY
[WBAN],
[YearMonthDay]


